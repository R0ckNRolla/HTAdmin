<html>
<head>
	<hta:application id="HTAdmin" applicationname="HTAdmin" icon="#" border="thick" caption="Yes" showintaskbar="Yes" sysmenu="Yes" windowstate="Normal">
	<title>HTAdmin</title>
	<script type="text/vbscript">
		' Force vbscript to declare all variables BEFORE we use them!
		' Helps find mistakes faster; Build good habits
		Option Explicit
		
		' AD UserFlag Constants
		Const ADS_UF_ACCOUNTDISABLE			= &H2
		Const ADS_UF_LOCKOUT				= &H10
		Const ADS_PASSWD_NOTREQD			= &H20
		Const ADS_PASSWD_CANT_CHANGE		= &H40
		Const ADS_UF_DONT_EXPIRE_PASSWD		= &H10000
		Const ADS_UF_PASSWORD_EXPIRED		= &H800000

		' Resize the window so it looks un-crappy
		window.resizeto 900,600
		
		' ----------------------------------------------------------------------
		' Is ACCOUNT a valid AD user account?
		' ----------------------------------------------------------------------
		Function isAccount( account )
			Dim aduser, retval

			' Try opening user account
			On Error Resume Next
			Set aduser = GetObject( "LDAP://" & account )
			If( IsEmpty(aduser) ) Then
				' Not valid, return False
				retval = False
			Else
				' Valid!  Return True
				retval = True
			End If

			' Return True/False
			isAccount = retval
		End Function

		' ----------------------------------------------------------------------
		' Enumerate group memberships
		' ----------------------------------------------------------------------
		Function getGroups( account )
			Dim aduser, groups, group
			
			Set aduser = GetObject( "LDAP://" & account )
			Set groups = aduser.Groups

			Set getGroups = groups
		End Function
		
		' ----------------------------------------------------------------------
		' Add user to group
		' ----------------------------------------------------------------------
		Function addGroup( account, group )
			Dim adgroup, aduser
			
			Set adgroup = GetObject( "LDAP://" & group )
			Set aduser = GetObject( "LDAP://" & account )
			adgroup.Add( aduser.ADsPath )
		End Function
		
		' ----------------------------------------------------------------------
		' Remove user from group
		' ----------------------------------------------------------------------
		Function removeGroup( account, group )
			Dim adgroup, aduser
			Const ADS_PROPERTY_DELETE = 4
			
			Set adgroup = GetObject( "LDAP://" & group )
			adgroup.PutEx ADS_PROPERTY_DELETE, "member", Array( account )
			adgroup.SetInfo
		End Function
		
		' ----------------------------------------------------------------------
		' Get user account control flag value
		' ----------------------------------------------------------------------
		Function getControlFlag( account, flag )
			Dim aduser, adflags, retval
			
			Set aduser = GetObject( "LDAP://" & account )
			adflags = aduser.get( "userAccountControl" )
			adflags = adflags AND flag

			' Is flag set?
			If adflags > 0 Then
				retval = True
			Else
				retval = False
			End If

			getControlFlag = retval
		End Function
		
		' ----------------------------------------------------------------------
		' Set user account control flag value
		' ----------------------------------------------------------------------
		Function setControlFlag( account, flag, value )
			Dim aduser, adflags
			
			' Get AD user object
			Set aduser = GetObject( "LDAP://" & account )
			adflags = aduser.get( "userAccountControl" )
			
			' Set flag True/False
			Select Case value
				Case True
					adflags = adflags OR flag
				Case False
					adflags = adflags AND NOT flag
			End Select
			
			' Save results back to AD
			aduser.put "userAccountControl", adflags
			aduser.SetInfo
		End Function

		' ----------------------------------------------------------------------
		' Disable user account
		' ----------------------------------------------------------------------
		Function disableAccount( account )
			setControlFlag account, ADS_UF_ACCOUNTDISABLE, True
		End Function
		
		' ----------------------------------------------------------------------
		' Enable user account
		' ----------------------------------------------------------------------
		Function enableAccount( account )
			setControlFlag account, ADS_UF_ACCOUNTDISABLE, False
		End Function
		
		' ----------------------------------------------------------------------
		' Lock user account
		' ----------------------------------------------------------------------
		Function lockAccount( account )
			setControlFlag account, ADS_UF_LOCKOUT, True
		End Function
		
		' ----------------------------------------------------------------------
		' Unlock user account
		' ----------------------------------------------------------------------
		Function unlockAccount( account )
			setControlFlag account, ADS_UF_LOCKOUT, False
		End Function
		
		' ----------------------------------------------------------------------
		' Get user attribute
		' ----------------------------------------------------------------------
		Function getAttribute( account, attrib )
			Dim aduser

			Set aduser = GetObject( "LDAP://" & account )
			msgbox aduser.Get( attrib )
		End Function
		
		' ----------------------------------------------------------------------
		' Set user attribute
		' ----------------------------------------------------------------------
		Function setAttribute( account, attrib, value )
			Dim aduser
			
			Set aduser = GetObject( "LDAP://" & account )
			aduser.put attrib, value
			aduser.SetInfo
		End Function

		' ----------------------------------------------------------------------
		' Change a user password
		' ----------------------------------------------------------------------
		Function changePassword( account, password )
			Dim aduser

			Set aduser = GetObject( "LDAP://" & account )
			aduser.SetPassword password
		End Function
		
		' ----------------------------------------------------------------------
		' Display panel of the GUI
		' ----------------------------------------------------------------------
		Function showPanel( panel )
			' Hide all panels
			editAccount.style.display = "none"
			createAccount.style.display = "none"
			bulkAccount.style.display = "none"
			settings.style.display = "none"
			welcome.style.display = "none"
			debugPanel.style.display = "none"
			
			' Unset selected tab
			document.getElementById("edit-tab").className = ""
			document.getElementById("create-tab").className = ""
			document.getElementById("bulk-tab").className = ""
			document.getElementById("settings-tab").className = ""
		
			' Show the appropriate panel
			Select Case panel
				Case "EDIT"
					editAccount.style.display = "block"
					document.getElementById("edit-tab").className = "selected"
				Case "CREATE"
					createAccount.style.display = "block"
					document.getElementById("create-tab").className = "selected"
				Case "BULK"
					bulkAccount.style.display = "block"
					document.getElementById("bulk-tab").className = "selected"
				Case "SETTINGS"
					settings.style.display = "block"
					document.getElementById("settings-tab").className = "selected"
				Case "DEBUG"
					debugPanel.style.display = "block"
			End Select
			
		End Function
		
		' ----------------------------------------------------------------------
		' Clear message
		' ----------------------------------------------------------------------
		Sub clearMessage
			message.innerHTML = ""
			message.style.display = "none"
		End Sub
		
		' ----------------------------------------------------------------------
		' Set informative message and display
		' ----------------------------------------------------------------------
		Function setMessage( messageText, style )
			
			' Set content of message box
			message.innerHTML = messageText
			
			' Change CSS class descriptor based on message type
			Select Case style
				Case "GOOD"		message.className = "response-good"
				Case "BAD"		message.className = "response-bad"
				Case "INFO"		message.className = "response-info"
			End Select
			
			' Display message box
			message.style.display = "block"

		End Function
		
				
	</script>
	<style type="text/css">
		<!--
			/* General styles */
			/* -------------------------------------------------------------------------------------- */
			body {
				margin-top: 30px;
				padding: 0px;
				background-color: #787878;
				font-family: sans-serif;
				color: #616161;
			}

			h1 {
				margin-left: 100px;
				padding-top: 55px;
			}

			/*#header {
				width: 775px;
				height: 130px;
				margin: 0 auto;
				background: url(data:image/gif;base64,) no-repeat left top;
				background-color: #fff;
			}*/
			
			#header {
				margin-left: 38px;
			}
			
			#header ul {
				list-style: none;
				padding: 0;
				margin: 0;
			}
			
			#header li {
				float: left;
			}

			#header span {
				display: none;
			}
			
			#edit-tab {
				width: 194px;
				height: 60px;
				background: url(data:image/png;base64,) no-repeat left top;
			}
			
			#edit-tab:hover, #edit-tab.selected {
				background: url(data:image/png;base64,) no-repeat left top;
			}
			
			#create-tab {
				width: 194px;
				height: 60px;
				background: url(data:image/png;base64,) no-repeat left top;
			}
			
			#create-tab:hover, #create-tab.selected {
				background: url(data:image/png;base64,) no-repeat left top;
				/*border-bottom: solid 2px #777976;*/
			}
			
			#bulk-tab {
				width: 194px;
				height: 60px;
				background: url(data:image/png;base64,) no-repeat left top;
			}
			
			#bulk-tab:hover, #bulk-tab.selected {
				background: url(data:image/png;base64,) no-repeat left top;
			}
			
			#settings-tab {
				width: 193px;
				height: 60px;
				background: url(data:image/png;base64,) no-repeat left top;
			}
			
			#settings-tab:hover, #settings-tab.selected {
				background: url(data:image/png;base64,) no-repeat left top;
			}
			
			#wrapper {
				width: 675px;
				background: #fff;
				margin: 0 auto;
				padding-top: 20px;
				padding-left: 50px;
				padding-right: 50px;
				padding-bottom: 0px;
				clear: both;
			}

			#footer {
				width: 775px;
				height: 30px;
				margin: 0 auto;
				background: url(data:image/gif;base64,) no-repeat left top;
			}

			.response-good {
				background: #90EE90;
				color: #006400;
				border: 1px dashed #006400;
				padding: 5px;
				margin-bottom: 15px;
			}

			.response-bad {
				background: #F08080;
				color: #8B0000;
				border: 1px dashed #8B0000;
				padding: 5px;
				margin-bottom: 15px;
			}
			
			.response-info {
				background: #F8F8B2;
				color: #968100;
				border: 1px dashed #968100;
				padding: 5px;
				margin-bottom: 15px;
			}

			.copyright{
				color: #ccc;
				padding-top: 5px;
				text-align: center;
				font-size: x-small;
				}

			/* CSS Nicify-Forms */
			/* Taken from Dynamic Drive CSS library
			/* http://www.dynamicdrive.com/style/
			/* -------------------------------------------------------------------------------------- */
			.cssform p {
				width: 335px;
				clear: left;
				margin: 0;
				padding: 5px 0 8px 0;
				padding-left: 250px; /*width of left column containing the label elements*/
				border-top: 1px dashed 	#808080;
				color: #8B0000;
				height: 1%;
			}

			.cssform label {
			/*	font-weight: bold;*/
				float: left;
				font-weight: bold;
				margin-left: -250px; /*width of left column*/
				width: 195px; /*width of labels. Should be smaller than left column (155px) to create some right margin*/
			}

			.cssform input[type="text"], input[type="password"], select {
				width: 325px; /*width of text boxes. IE6 does not understand this attribute*/
			}

			.cssform input[type="submit"], input[type="button"] {
				float: right;
				margin-left: 5px;
			}
		-->
	</style>
</head>

<body>
	<div id="header">
		<ul>
			<li id="edit-tab" onclick="showPanel 'EDIT'"><span>Edit Account</span>
			<li id="create-tab" onclick="showPanel 'CREATE'"><span>Create Account</span>
			<li id="bulk-tab" onclick="showPanel 'BULK'"><span>Bulk Upload</span>
			<li id="settings-tab" onclick="showPanel 'SETTINGS'"><span>Settings</span>
		</ul>
	</div>
	<div id="wrapper">

		<!-- Message placeholder -->
		<div id="message" style="display: none"></div>

		<!-- Debug panel -->
		<div id="debugPanel" style="display:none">
			<a onclick="clearMessage">Reset</a>
			<a onclick="setMessage 'Hurray!  A success message!', 'GOOD'">Good</a>
			<a onclick="setMessage 'Boo!  A failure message! :-(', 'BAD'">Bad</a>
			<a onclick="setMessage 'An informative message.  I have no strong feelings either way about this message.', 'INFO'">Info</a>
			<br>
			<a onclick="If isAccount('CN=Test User,OU=Users-Tech,DC=west,DC=k12,DC=oh,DC=us') Then msgbox('Valid Account!') Else msgbox('Invalid Account!')">Valid account</a>
			<a onclick="If isAccount('cn=Your Mother,ou=Users-Tech,dc=west,dc=k12,dc=oh,dc=us') Then msgbox('Valid Account!') Else msgbox('Invalid Account!')">Invalid account</a>
			<br>
			<a onclick="setAttribute 'CN=Test User,OU=Users-Tech,DC=west,DC=k12,DC=oh,DC=us', 'sn', 'Deacon': getAttribute 'CN=Test User,OU=Users-Tech,DC=west,DC=k12,DC=oh,DC=us', 'sn'">Set given name to "Deacon"</a>
			<a onclick="setAttribute 'CN=Test User,OU=Users-Tech,DC=west,DC=k12,DC=oh,DC=us', 'sn', 'User': getAttribute 'CN=Test User,OU=Users-Tech,DC=west,DC=k12,DC=oh,DC=us', 'sn'">Set given name to "User"</a>
			<br>
			<a onclick="Dim groups, group : Set groups = getGroups( 'CN=Test User,OU=Users-Tech,DC=west,DC=k12,DC=oh,DC=us' ) : For Each group in groups : msgbox group.get('cn') : Next">List groups</a>
			<br>
			<a onclick="addGroup 'CN=Test User,OU=Users-Tech,DC=west,DC=k12,DC=oh,DC=us', 'CN=Test Group,OU=Users-Tech,DC=west,DC=k12,DC=oh,DC=us'">Add to group</a>
			<a onclick="removeGroup 'CN=Test User,OU=Users-Tech,DC=west,DC=k12,DC=oh,DC=us', 'CN=Test Group,OU=Users-Tech,DC=west,DC=k12,DC=oh,DC=us'">Remove from group</a>
			<br>
			<a onclick="If getControlFlag('CN=Test User,OU=Users-Tech,DC=west,DC=k12,DC=oh,DC=us', ADS_UF_ACCOUNTDISABLE) Then msgbox('True!') Else msgbox('False!')">Is account disabled?</a>
			<a onclick="enableAccount 'CN=Test User,OU=Users-Tech,DC=west,DC=k12,DC=oh,DC=us'">Enable</a>
			<a onclick="disableAccount 'CN=Test User,OU=Users-Tech,DC=west,DC=k12,DC=oh,DC=us'">Disable</a>
			<br>
			<a onclick="If getControlFlag('CN=Test User,OU=Users-Tech,DC=west,DC=k12,DC=oh,DC=us', ADS_UF_LOCKOUT) Then msgbox('True!') Else msgbox('False!')">Is account locked?</a>
			<a onclick="unlockAccount 'CN=Test User,OU=Users-Tech,DC=west,DC=k12,DC=oh,DC=us'">Unlock</a>
			<br><br>
		</div>
	
		<!-- Account editing panel -->
		<div id="editAccount" style="display: none">
			<form class="cssform" name="findUser">
				<input type="text" name="find" id="find">
				<input type="button" value="Find User" onclick="findUser()">
			</form>
		</div>

		<!-- Account creation panel -->
		<div id="createAccount" style="display: none">
			<form class="cssform" name="newuserform">
				<p><label for="fname">First Name</label>
				<input type="text" name="fname" id="fname"></p>
				<p><label for="lname">Last Name</label>
				<input type="text" name="lname" id="lname"></p>
				<p><label for="gyear">Graduation Year</label>
				<input type="text" name="gyear" id="gyear"></p>
				<p><label for="pass">Password</label>
				<input type="text" name="pass" id="pass"></p>
				<p><label for="school">School</label>
				<select id="school">
					<option value="one">School One</option>
					<option value="two">School Two</option>
					<option value="three">School Three</option>
				</select>
				<p><input type="button" value="Create User" onclick="createNewUser()"></p>
			</form>
		</div>
		
		<!-- Bulk account creation panel -->
		<div id="bulkAccount" style="display: none">
			Bulk account creation
		</div>
		
		<!-- Settings panel -->
		<div id="settings" style="display: none">
			Settings
		</div>
		
		<!-- Welcome panel -->
		<div id="welcome">
			Welcome!
		</div>
		
	</div>
	<div id="footer"></div>
	<div class="copyright" onclick="showPanel 'DEBUG'">&copy;2013, William Deacon</div>
</body>
</html>
